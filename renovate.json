{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "gomod": {
    "postUpdateOptions": [
      "gomodTidyAll"
    ]
  },
  "packageRules": [
    {
      "matchManagers": ["gomod"],
      "postUpdateOptions": [
        "gomodTidyAll"
      ]
    }
  ],
  "enabledManagers": ["gomod"],
  "schedule": ["at any time"],
  "timezone": "UTC",
  "prHourlyLimit": 10,
  "prConcurrentLimit": 5,
  "branchPrefix": "renovate-test-"
}
